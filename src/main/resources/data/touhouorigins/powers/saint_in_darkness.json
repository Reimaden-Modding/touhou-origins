{
  "type": "apoli:multiple",
  "blindness_immunity": {
    "type": "apoli:effect_immunity",
    "effects": [
      "minecraft:blindness",
      "minecraft:darkness"
    ]
  },
  "resource": {
    "type": "apoli:resource",
    "min": 0,
    "max": 2
  },
  "toggle": {
    "type": "apoli:active_self",
    "entity_action": {
      "type": "apoli:if_else_list",
      "actions":
      [
        {
          "condition": {
            "type": "apoli:and",
            "conditions":
            [
              {
                "type": "apoli:resource",
                "resource": "*:*_resource",
                "comparison": "==",
                "compare_to": 1
              }
            ]
          },
          "action": {
            "type": "apoli:and",
            "actions":
            [
              {
                "type": "apoli:change_resource",
                "resource": "*:*_resource",
                "operation": "set",
                "change": 0
              }
            ]
          }
        },
        {
          "condition": {
            "type": "apoli:and",
            "conditions":
            [
              {
                "type": "apoli:resource",
                "resource": "*:*_resource",
                "comparison": "==",
                "compare_to": 0
              }
            ]
          },
          "action": {
            "type": "apoli:and",
            "actions":
            [
              {
                "type": "apoli:change_resource",
                "resource": "*:*_resource",
                "operation": "set",
                "change": 1
              },
              {
                "type": "apoli:trigger_cooldown",
                "power": "*:*_splat_warmup"
              }
            ]
          }
        },
        {
          "condition": {
            "type": "apoli:resource",
            "resource": "*:*_resource",
            "comparison": "==",
            "compare_to": 2
          },
          "action": {
            "type": "apoli:execute_command",
            "command": "title @s actionbar {\"translate\":\"power.touhouorigins.midnight_bird_cooldown\"}"
          }
        }
      ]
    }
  },
  "propel_forward": {
    "type": "apoli:action_over_time",
    "entity_action": {
      "type": "apoli:and",
      "actions":
      [
        {
          "type": "apoli:add_velocity",
          "space": "local",
          "set": true,
          "z": 1
        }
      ]
    },
    "interval": 2,
    "condition": {
      "type": "apoli:and",
      "conditions":
      [
        {
          "type": "apoli:sneaking",
          "inverted": true
        },
        {
          "type": "apoli:resource",
          "resource": "*:*_resource",
          "comparison": "==",
          "compare_to": 1
        }
      ]
    }
  },
  "reset_splat_on_sneak": {
    "type": "apoli:action_over_time",
    "entity_action": {
      "type": "apoli:and",
      "actions": [
        {
          "type": "apoli:add_velocity",
          "space": "local",
          "set": true,
          "y": 0
        },
        {
          "type": "apoli:change_resource",
          "resource": "*:*_splat_warmup",
          "change": 10
        }
      ]
    },
    "interval": 1,
    "condition": {
      "type": "apoli:and",
      "conditions":
      [
        {
          "type": "apoli:sneaking"
        },
        {
          "type": "apoli:resource",
          "resource": "*:*_resource",
          "comparison": "==",
          "compare_to": 1
        }
      ]
    }
  },
  "cancel_gravity": {
    "type": "apoli:modify_falling",
    "velocity": 0,
    "take_fall_damage": false,
    "condition": {
      "type": "apoli:resource",
      "resource": "*:*_resource",
      "comparison": "==",
      "compare_to": 1
    }
  },
  "anti_anti_cheat": {
    "type": "apoli:action_over_time",
    "rising_action": {
      "type": "apoli:grant_power",
      "power": "*:can_fly",
      "source": "*:kurayami"
    },
    "falling_action": {
      "type": "apoli:revoke_power",
      "power": "*:can_fly",
      "source": "*:kurayami"
    },
    "condition": {
      "type": "apoli:resource",
      "resource": "*:*_resource",
      "comparison": "==",
      "compare_to": 1
    }
  },
  "splat": {
    "type": "apoli:damage_over_time",
    "interval": 20,
    "onset_delay": 1,
    "damage": 1,
    "damage_source": {
      "name": "flyIntoWall",
      "unblockable": true
    },
    "condition": {
      "type": "apoli:and",
      "conditions":
      [
        {
          "type": "apoli:resource",
          "resource": "*:*_resource",
          "comparison": "==",
          "compare_to": 1
        },
        {
          "type": "apoli:sneaking",
          "inverted": true
        },
        {
          "type": "apoli:or",
          "conditions":
          [
            {
              "type": "apoli:block_collision",
              "offset_x": 0.1,
              "offset_y": 0.1,
              "offset_z": 0.1
            },
            {
              "type": "apoli:block_collision",
              "offset_x": -0.1,
              "offset_y": -0.1,
              "offset_z": -0.1
            }
          ]
        },
        {
          "type": "apoli:resource",
          "resource": "*:*_splat_warmup",
          "comparison": "==",
          "compare_to": 0
        }
      ]
    }
  },
  "splat_warmup": {
    "type": "apoli:cooldown",
    "cooldown": 10
  },
  "toggle_splat": {
    "type": "apoli:action_when_damage_taken",
    "entity_action": {
      "type": "apoli:and",
      "actions":
      [
        {
          "type": "apoli:change_resource",
          "resource": "*:*_resource",
          "operation": "set",
          "change": 2
        },
        {
          "type": "apoli:trigger_cooldown",
          "power": "*:*_splat_recover_timer"
        }
      ]
    }
  },
  "splat_recover": {
    "type": "apoli:action_over_time",
    "entity_action": {
      "type": "apoli:change_resource",
      "resource": "*:*_resource",
      "operation": "set",
      "change": 0
    },
    "interval": 1,
    "condition": {
      "type": "apoli:or",
      "conditions": [
        {
          "type": "apoli:resource",
          "resource": "*:*_splat_recover_timer",
          "comparison": "==",
          "compare_to": 1
        }
      ]
    }
  },
  "splat_recover_timer": {
    "type": "apoli:cooldown",
    "cooldown": 10,
    "hud_render": {
      "should_render": true,
      "sprite_location": "origins:textures/gui/community/spiderkolo/resource_bar_03.png",
      "bar_index": 23
    }
  },
  "flight_sound": {
    "type": "apoli:active_self",
    "entity_action": {
      "type": "apoli:play_sound",
      "sound": "touhouorigins:entity.kurayami.flight"
    },
    "condition": {
      "type": "apoli:and",
      "conditions":
      [
        {
          "type": "apoli:resource",
          "resource": "*:*_resource",
          "comparison": "<=",
          "compare_to": 1
        }
      ]
    }
  },
  "is_that_so": {
    "type": "apoli:active_self",
    "key": {
      "key": "key.origins.secondary_active"
    },
    "entity_action": {
      "type": "apoli:and",
      "actions": [
        {
          "type": "apoli:area_of_effect",
          "radius": 16,
          "include_target": true,
          "bientity_action": {
            "type": "apoli:target_action",
            "action": {
              "type": "apoli:and",
              "actions":
              [
                {
                  "type": "apoli:apply_effect",
                  "effect": {
                    "effect": "minecraft:blindness",
                    "duration": 100,
                    "show_particles": false,
                    "show_icon": true
                  }
                },
                {
                  "type": "apoli:extinguish"
                }
              ]
            }
          }
        },
        {
          "type": "apoli:play_sound",
          "sound": "touhouorigins:entity.kurayami.darkness"
        },
        {
          "type": "apoli:execute_command",
          "command": "fill ~-8 ~-1 ~-8 ~8 ~1 ~8 minecraft:air replace minecraft:fire",
          "condition": {
            "type": "apoli:block_in_radius",
            "block_condition": {
              "type": "apoli:block",
              "block": "minecraft:fire"
            },
            "radius": 16,
            "shape": "cube",
            "comparison": ">=",
            "compare_to": 1
          }
        }
      ]
    },
    "cooldown": 200,
    "hud_render": {
      "should_render": true,
      "sprite_location": "origins:textures/gui/community/spiderkolo/resource_bar_03.png",
      "bar_index": 18
    }
  },
  "chomp": {
    "type": "apoli:action_on_hit",
    "bientity_action": {
      "type": "apoli:if_else_list",
      "actions": [
        {
          "condition": {
            "type": "apoli:target_condition",
            "condition": {
              "type": "apoli:and",
              "conditions": [
                {
                  "type": "apoli:relative_health",
                  "comparison": "<=",
                  "compare_to": 0
                },
                {
                  "type": "apoli:in_tag",
                  "tag": "touhouorigins:meatless",
                  "inverted": true
                },
                {
                  "type": "apoli:or",
                  "conditions" : [
                    {
                      "type": "origins:origin",
                      "origin": "touhouorigins:ghost"
                    },
                    {
                      "type": "origins:origin",
                      "origin": "touhouorigins:vengeful_spirit"
                    },
                    {
                      "type": "origins:origin",
                      "origin": "touhouorigins:tsukumogami"
                    }
                  ],
                  "inverted": true
                }
              ]
            }
          },
          "action": {
            "type": "apoli:and",
            "actions": [
              {
                "type": "apoli:damage",
                "amount": 8,
                "source": {
                  "name": "chomp",
                  "bypasses_armor": true,
                  "unblockable": true
                }
              },
              {
                "type": "apoli:actor_action",
                "action": {
                  "type": "apoli:and",
                  "actions": [
                    {
                      "type": "apoli:play_sound",
                      "sound": "touhouorigins:entity.kurayami.chomp"
                    },
                    {
                      "type": "apoli:heal",
                      "amount": 20
                    },
                    {
                      "type": "apoli:feed",
                      "food": 20,
                      "saturation": 0.25
                    },
                    {
                      "type": "apoli:apply_effect",
                      "effect": {
                        "effect": "minecraft:absorption",
                        "duration": 600,
                        "amplifier": 1,
                        "show_particles": false,
                        "show_icon": false
                      }
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "condition": {
            "type": "apoli:and",
            "conditions": [
              {
                "type": "apoli:target_condition",
                "condition": {
                  "type": "apoli:and",
                  "conditions": [
                    {
                      "type": "apoli:relative_health",
                      "comparison": ">",
                      "compare_to": 0
                    },
                    {
                      "type": "apoli:in_tag",
                      "tag": "touhouorigins:meatless",
                      "inverted": true
                    },
                    {
                      "type": "apoli:or",
                      "conditions" : [
                        {
                          "type": "origins:origin",
                          "origin": "touhouorigins:ghost"
                        },
                        {
                          "type": "origins:origin",
                          "origin": "touhouorigins:vengeful_spirit"
                        },
                        {
                          "type": "origins:origin",
                          "origin": "touhouorigins:tsukumogami"
                        }
                      ],
                      "inverted": true
                    }
                  ]
                }
              }
            ]
          },
          "action": {
            "type": "apoli:and",
            "actions": [
              {
                "type": "apoli:damage",
                "amount": 8,
                "source": {
                  "name": "chomp",
                  "bypasses_armor": true,
                  "unblockable": true
                }
              },
              {
                "type": "apoli:actor_action",
                "action": {
                  "type": "apoli:and",
                  "actions": [
                    {
                      "type": "apoli:play_sound",
                      "sound": "touhouorigins:entity.kurayami.chomp"
                    },
                    {
                      "type": "apoli:apply_effect",
                      "effect": {
                        "effect": "minecraft:absorption",
                        "duration": 600,
                        "amplifier": 1,
                        "show_particles": false,
                        "show_icon": false
                      }
                    },
                    {
                      "type": "apoli:feed",
                      "food": 1,
                      "saturation": 1
                    }
                  ]
                }
              }
            ]
          }
        }
      ]
    },
    "condition": {
      "type": "apoli:equipped_item",
      "equipment_slot": "mainhand",
      "item_condition": {
        "type": "apoli:empty"
      }
    },
    "bientity_condition": {
      "type": "apoli:target_condition",
      "condition": {
        "type": "apoli:status_effect",
        "effect": "minecraft:blindness"
      }
    },
    "cooldown": 200
  },
  "give_origin_tag": {
    "type": "origins:action_on_callback",
    "entity_action_added": {
      "type": "apoli:execute_command",
      "command": "tag @s add kurayami_ex_origin"
    },
    "entity_action_lost": {
      "type": "apoli:execute_command",
      "command": "tag @s remove kurayami_ex_origin"
    }
  },
  "black_smoke": {
    "type": "apoli:action_over_time",
    "entity_action": {
      "type": "apoli:and",
      "actions": [
        {
          "type": "apoli:execute_command",
          "command": "particle minecraft:dust 0 0 0 1 ~ ~0.5 ~ .5 .1 .5 0.2 5 force @e[tag=kurayami_ex_origin,type=minecraft:player]"
        },
        {
          "type": "apoli:execute_command",
          "command": "particle minecraft:dust 0 0 0 1.5 ~ ~1 ~ 1 1 1 0.2 100 force @e[tag=!kurayami_ex_origin,type=minecraft:player]"
        },
        {
          "type": "apoli:area_of_effect",
          "radius": 16,
          "bientity_action": {
            "type": "apoli:target_action",
            "action": {
              "type": "apoli:apply_effect",
              "effect": {
                "effect": "minecraft:darkness",
                "duration": 100,
                "show_particles": true,
                "show_icon": true
              }
            }
          }
        }
      ]
    },
    "interval": 1
  },
  "decrease_food_value": {
    "type": "apoli:modify_food",
    "item_condition": {
      "type": "apoli:and",
      "conditions": [
        {
          "type": "apoli:food"
        },
        {
          "type": "apoli:ingredient",
          "ingredient": {
            "tag": "touhouorigins:raw_meat"
          },
          "inverted": true
        }
      ]
    },
    "food_modifier": {
      "name": "Crude Appetite food penalty",
      "operation": "multiply_total",
      "value": -0.5
    },
    "saturation_modifier": {
      "name": "Crude Appetite saturation penalty",
      "operation": "multiply_total",
      "value": -0.75
    }
  },
  "increase_food_value": {
    "type": "apoli:modify_food",
    "item_condition": {
      "type": "apoli:and",
      "conditions": [
        {
          "type": "apoli:food"
        },
        {
          "type": "apoli:ingredient",
          "ingredient": {
            "tag": "touhouorigins:raw_meat"
          }
        }
      ]
    },
    "food_modifier": {
      "name": "Crude Appetite food bonus",
      "operation": "multiply_total",
      "value": 0.75
    },
    "saturation_modifier": {
      "name": "Crude Appetite saturation bonus",
      "operation": "multiply_total",
      "value": 0.50
    },
    "prevent_effects": true
  },
  "badges":
  []
}
